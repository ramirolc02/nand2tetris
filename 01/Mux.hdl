/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
// (Not sel and a) or (sel and b)    
    Not(in=sel,out=notsel);
    And(a=a , b=notsel , out=andout1 );
    And(a=sel, b=b , out=andout2 );
    Or(a=andout1, b=andout2 , out=out );
    }